<cfsetting showdebugoutput="false" ><cfparam name="form.task_note_id" type="numeric" default=-1><cfparam name="url.task_note_id" type="string" default=""><cfparam name="url.task_id" type="string" default=""><cfparam name="url.project_id" type="numeric" default=-1><cfif form.task_note_id neq -1>		<cfset application._model_task.delete_task_note(form.task_note_id)></cfif><cfif url.task_note_id neq "">		<cfset application._model_task.delete_task_note(INT(url.task_note_id))>	<cflocation url="task.cfm?project_id=#url.project_id#&task_id=#url.task_id###commits" addtoken="false"></cfif>
