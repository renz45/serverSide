<cfsetting showdebugoutput="false" ><cfparam name="url.project_id" type="numeric" default=-1><cfparam name="url.user_id" type="numeric" default=-1><cfif url.project_id neq -1 and url.user_id neq -1>	<cfset project_id = application._model_user.add_to_portfolio(url.user_id,url.project_id)>	</cfif><cflocation url="project.cfm?project_id=#url.project_id#" addtoken="false" >
